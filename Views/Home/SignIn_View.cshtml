@{
    ViewBag.Title = "SignIn";
}
<link  rel="stylesheet" type="text/css" href="~/Content/Common_StyleSheet.css"/>
<script type="text/javascript" src="~/Scripts/Jquery1.9.1.js"></script>
<script type="text/javascript" src="~/Scripts/CommonScript.js"></script>
<h2 class="site-title">SignIn</h2>
<div id="invalidMessageDiv" class="errorMessage" hidden="hidden">
     invalid username or password try again...
        </div>
<form name="SignInForm" id="SignInForm" onsubmit="return signWithValuesInPressed();" enctype="multipart/form-data"
     method="post">
    
<div class="main" >
    
       <input type="text" class="textfieldclass" placeholder="Enter PhoneNo Here" 
           name="phoneno" id="phoneno" style="width:350px;margin-top:80px;"/>
    <br/>
        <input type="password" class="textfieldclass inSpacing" placeholder="Enter Password Here" 
            name="password" id="password" style="width:350px" />
    <input type="submit" id="signIn" value="SignIn"  class="buttonclass inSpacing" style="width:350px;"
       />
    </div>
</form>