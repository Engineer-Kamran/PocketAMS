@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
     <link rel="stylesheet" type="text/css" href="~/Content/Common_StyleSheet.css"/>
     <link rel="stylesheet" type="text/css" href="~/Content/Site.css"/>
     <script type="text/javascript" src="~/Scripts/Jquery1.9.1.js"></script>
    <script type="text/javascript" src="~/Scripts/CommonScript.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <h2 class="site-title">Student Query</h2>
<div class="main">
<form name="queryForm" id="queryForm" onsubmit="return queryPressed();" enctype="multipart/form-data"
     method="post">

 <input type="text" class="textfieldclass" placeholder="Sender" 
           name="sender" id="sendto" style="width:350px;margin-top:30px;"/>
    <input type="text" class="textfieldclass" placeholder="Receiver" 
           name="receiver" id="receiver" style="width:350px;"/>
 <input type="text" class="textfieldclass" placeholder="Subject" 
            name="subject" id="subject" style="width:350px" /> 
    <textarea name="detail" cols="2" rows="5" 
        style="margin-left:70px;border-radius:10px;width:350px "  placeholder="Enter your problem here" ></textarea>   
        <input type="submit" id="query" value="Send"  class="buttonclass" style="width:350px;"
       />
</form>    
</div>

</body>
</html>
